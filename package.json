    "@swc/jest": "^0.2.24",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.4",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "jest-watch-typeahead": "^1.0.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@swc-node/register": "^1.6.2"
